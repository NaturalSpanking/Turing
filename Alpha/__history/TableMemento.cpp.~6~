int TableMemento::SaveToFile(std::vector<std::vector<rule> > table, char* pathToFile)
{
	ofstream OutputFile;
	OutputFile.open(pathToFile, ios::binary);
	if(!OutputFile.is_open()) return 1;

	return 0;
}

